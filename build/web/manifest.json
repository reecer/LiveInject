{
  "manifest_version": 2,
  
  "name": "LiveInject",
  "description": "",
  "version": "0.0.0",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "debugger", "tabs"
  ],
  "background":{
    "page": "index.html"
  },
  "browser_action":{
    "default_icon": "icons/stopped.png",
    "default_title": "Enable LiveInject"
  },
  "options_page": "options.html"
}